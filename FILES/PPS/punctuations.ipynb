      "source": [
        "punctuations = \"()!,.<>/\"\n",
        "\n",
        "test_str = \"Hi , This ,is ,JACK. (Trainer/ Developer) !\"\n",
        "\n",
        "clean_str = \"\"\n",
        "\n",
        "for char in test_str:\n",
        "    if char in punctuations:\n",
        "        continue\n",
        "    else:\n",
        "        clean_str += char\n",
        "\n",
        "print(clean_str)"
      ]
    }
  ]
